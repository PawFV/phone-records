{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "typeRoots": ["./node_modules/@types"],
    "paths": {}
  }
}
